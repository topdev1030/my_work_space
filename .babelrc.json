{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 100
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-private-property-in-object"],
  "env": {
    "development": {
      "plugins": [["react-refresh/babel", { "skipEnvCheck": true }]]
    }
  }
}

